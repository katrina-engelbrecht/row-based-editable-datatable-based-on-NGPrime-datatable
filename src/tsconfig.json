    "lib": ["es6", "dom"],
    "mapRoot": "./",
    "module": "es6",
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "./typings",
      "../node_modules/@types"
    ]
  },
  "include": [
    "./typings",
    "../node_modules/@types",
    "./"
  ]
}

